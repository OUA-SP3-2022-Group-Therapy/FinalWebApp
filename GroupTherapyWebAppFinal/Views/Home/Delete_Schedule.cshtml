@model GroupTherapyWebAppFinal.Models.Schedule

@{
    ViewData["Title"] = "Delete Schedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int UserID = ViewBag.UserID;
    int GroupID = ViewBag.GroupID;
}
}

<h1>Delete Schedule</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Schedule</h4>
    <hr />   

    <form asp-action="DeleteSchedule">
        <div class="form-group">
                <label asp-for="ScheduleID" class="control-label"></label>
                <input asp-for="ScheduleID" class="form-control" />
                <span asp-validation-for="ScheduleID" class="text-danger"></span>
        </div>   
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</div>